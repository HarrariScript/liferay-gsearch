{namespace GSearchImageResult}

/**
 * Print one image result item (Image search).
 *
 * @param item
 */
{template .imageItem}
	<div class="image">
		{if $item.imageSrc != ''}
			<a href="{$item.link}">
				<img alt="{$item.title}" src="{$item.imageSrc}" title="{$item.title}" />
			</a>
		{/if}
		<div class="meta">
		</div>			
	</div>
{/template}
